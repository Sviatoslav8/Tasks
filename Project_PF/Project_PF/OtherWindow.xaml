<Window x:Class="Project_PF.OtherWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_PF"
        mc:Ignorable="d"
        Title="OtherWindow" Height="450" Width="800">
    <StackPanel Background="Aqua" Orientation="Horizontal">
        <TextBlock Background="White" Margin="5" Padding="5" TextAlignment="Center">text</TextBlock>
        <TextBlock Background="Black" Margin="5" Padding="5" TextAlignment="Center">text</TextBlock>
        <TextBlock Background="Magenta" Margin="5" Padding="5" TextAlignment="Center">text</TextBlock>
        <TextBlock Background="Yellow" Margin="5" Padding="5" TextAlignment="Center">text</TextBlock>
        <TextBlock Background="Green" Margin="5" Padding="5" TextAlignment="Center">text</TextBlock>
        <TextBlock Background="OrangeRed" Margin="5" Padding="5" TextAlignment="Center">text</TextBlock>
    </StackPanel>
</Window>
